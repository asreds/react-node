{
  "name": "react-node",
  "version": "1.0.0",
  "description": "React and node js server side rendering",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server:watch\"",
    "server:watch": "nodemon --watch src/server/* --watch assets/ --exec 'node -r dotenv/config -r babel-register' ./src/server/index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asreds/react-node.git"
  },
  "keywords": [
    "react",
    "js",
    "node",
    "js",
    "serverside",
    "rendering"
  ],
  "author": "@asreds",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asreds/react-node/issues"
  },
  "homepage": "https://github.com/asreds/react-node#readme",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.6.1",
    "dotenv": "^6.0.0"
  }
}
